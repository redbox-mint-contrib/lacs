{
    "harvester": {
        "type": "csv",
        "csv": {
            "fileLocation": "${fascinator.home}/data/StaffList.csv",
            "idColumn": "ID",
            "recordIDPrefix": "",
            "headerRow": true,
            "headerList": ["ID", "Honorific", "First Name", "Last Name", "Division", "School"],
            "delimiter": ",",
            "ignoredFields": [],
            "includedFields" : ["ID", "Honorific", "First_Name", "Last_Name", "Division", "School"]
        }
    },
    "indexer": {
        "script": {
            "type": "python",
            "rules": "staff-sample.py"
        },
        "params": {
            "repository.name": "Staff",
            "repository.type": "Staff"
        }
    }
}